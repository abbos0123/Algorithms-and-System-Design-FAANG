class Solution {
   public  int[] successfulPairs(int[] spells, int[] potions, long success) {
           int n = spells.length, m = potions.length;
           Arrays.sort(potions);
           
           for(int i =0; i < n; i++){
              int low = 0, high = m;         
               while (low < high){
                   int mid = low + (high - low)/2;
                   
                   if((long)potions[mid] * spells[i] >= success)
                       high = mid;
                   else 
                       low = mid + 1;  
               }
               
               spells[i] = m - low;
           }
           
        return spells;
       }
}
